{
  "name": "immedine",
  "version": "1.0.0",
  "description": "ImmeDine",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=development PORT=8000 node --trace-warnings app.js",
    "watch:windows": "SET NODE_ENV=development&& SET DISABLE_CRON=false&& nodemon app.js",
    "watch": "NODE_ENV=development && DISABLE_CRON=false && nodemon app.js",
    "server": "nodemon app.js",
    "watch-test": "NODE_ENV=testing PRETTY_DEBUG=true DISABLE_CRON=true PORT=8000 nodemon app.js",
    "start-build": "gulp build",
    "build": "./node_modules/.bin/gulp",
    "lint": "./node_modules/.bin/gulp lint",
    "commit": "git cz",
    "lint-watch": "./node_modules/.bin/gulp lintWatch",
    "generate-jsdoc": "rm -rf ./api/docs/jsdoc/render/* && ./node_modules/.bin/jsdoc -c jsdoc-config.json --readme README.md -r api/controllers api/modules -d ./api/docs/jsdoc/render/",
    "format": "prettier --write ."
  },
  "engineStrict": true,
  "engines": {
    "node": "18"
  },
  "author": "Goutam",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "babel-loader": "^7.1.5",
    "babelify": "^10.0.0",
    "gulp": "^4.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-jshint": "^2.1.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.6.5",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "*",
    "js-yaml": "^3.11.0",
    "jsdoc": "^4.0.2",
    "jshint": "^2.12.0",
    "nodemon": "^2.0.4",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "apn": "^2.2.0",
    "aws-sdk": "^2.1209.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fcm-node": "^1.6.1",
    "firebase-admin": "^13.3.0",
    "fs-extra": "*",
    "gpxparser": "^3.0.8",
    "helmet": "^7.1.0",
    "html-pdf": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "moment": "^2.29.4",
    "mongoose": "^7.6.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.12",
    "node-cron": "^3.0.3",
    "pug": "^3.0.2",
    "puid": "^1.0.7",
    "revalidator": "^0.3.1",
    "xlsx": "^0.18.5"
  }
}
